from flask import Flask, jsonify
import requests 
import json
import JsonArray

resto = [{'uno': 2,'tre': 'quattro'},{'uno': 6, 'tre': 'otto'} ]
qui= 'hola'


app = Flask(__name__)

@app.route('/')
def index():
    res = [{'uno': 4,'tre': 'abishnow'}]
    ris = jsonify(res)
    ris.headers.add("Access-Control-Allow-Origin", "*")
    return ris
    
@app.route("/je", methods=['GET'])	
def getje():
    ris = jsonify(resto)
    ris.headers.add("Access-Control-Allow-Origin", "*")
    return ris
    
  
    
@app.route("/jo", methods=['GET'])
def getJo():
    risposta = JsonArray.dammi()
    risposta_finale = jsonify(risposta)
    risposta_finale.headers.add("Access-Control-Allow-Origin", "*")
    return risposta_finale
    
if __name__ == "__main__":
    app.run(debug=True)
